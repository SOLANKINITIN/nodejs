<section class="user-section">
    <div class="containe">

        {{!-- You have to start with this to make Search --}}



        <div class="row ">
            <div class="info">
                <div class="input-box " id="search-input">
                    <form>
                        <input type="text" placeholder="Search Here" name="search">
                    </form>
                </div>
            </div>
        </div>

        <!--new row-->

        <div class="block-section">
            <div class="row">
            <!--second -->
            {{# each users}}
            <div class="block-section">
                <div class="row">
                    <div class="col s12 m12">
                        <div class="row-content">
                            <div>
                                <p>{{this.username}}</p>
                            </div>
                            <div>
                                <label>
                                    <input type="checkbox" class="filled-in checkbox-blue" checked="checked" />
                                    <span class="block filled-in-box" id="block">Block</span>
                                </label>
                            </div>
                            <div>
                                <label>
                                    <input type="checkbox" class="filled-in checkbox-blue" checked="checked" />
                                    <span class="Admin filled-in-box" id="Admin">Admin</span>
                                </label>
                            </div>
                            <div>
                                <a href="/user/delete/{{this._id}}"><button class="btn btn-blue"> Delete</button></a>
                            </div>
                        </div>
                    </div>
                </div>
                {{/each}}
               
</section>